{
    "multipart": [
        {   "apply": { "model": "vanillaextended:block/transport_pipe_base" }},
		{   "when": { "up": "true" },
            "apply": { "model": "vanillaextended:block/transport_pipe_side", "uvlock": true }
        },
		{   "when": { "down": "true" },
            "apply": { "model": "vanillaextended:block/transport_pipe_side", "uvlock": true }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "vanillaextended:block/transport_pipe_side", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "vanillaextended:block/transport_pipe_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "vanillaextended:block/transport_pipe_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "vanillaextended:block/transport_pipe_side", "y": 270, "uvlock": true }
        }
    ]
}
